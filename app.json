{
  "expo": {
    "name": "Hakerin",
    "slug": "Hakerin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "fonts": {
      "Estedad-Black": "./assets/fonts/Estedad-Black.ttf",
      "Estedad-Bold": "./assets/fonts/Estedad-Bold.ttf",
      "Estedad-ExtraBold": "./assets/fonts/Estedad-ExtraBold.ttf",
      "Estedad-ExtraLight": "./assets/fonts/Estedad-ExtraLight.ttf",
      "Estedad-Light": "./assets/fonts/Estedad-Light.ttf",
      "Estedad-Medium": "./assets/fonts/Estedad-Medium.ttf",
      "Estedad-Regular": "./assets/fonts/Estedad-Regular.ttf",
      "Estedad-SemiBold": "./assets/fonts/Estedad-SemiBold.ttf",
      "Estedad-Thin": "./assets/fonts/Estedad-Thin.ttf"
    }
  }
}
